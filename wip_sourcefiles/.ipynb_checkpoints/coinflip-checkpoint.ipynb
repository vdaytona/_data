    "# Simulate betting\n",
    "def simulate_bet(results_context, reverser, stake):\n",
    "    equity = []\n",
    "    equity.append(0)\n",
    "    for i in range(1, len(results_context)):\n",
    "        if results_context[i-1] >= reverser:\n",
    "            if results_context[i] >= reverser:\n",
    "                equity.append(equity[-1] - stake)\n",
    "            else:\n",
    "                equity.append(equity[-1] + stake)\n",
    "    return equity"
